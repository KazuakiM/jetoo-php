{
    "name": "kazuakim/jetoo",
    "description": "Cross-language URL classifier library, PHP implementation",
    "keywords": [
        "url"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Kazuaki Mabuchi",
            "email": "kazuakim.public@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.3.3"
    },
    "autoload": {
        "psr-4": {
            "Jetoo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetoo\\": "tests/Jetoo/"
        }
    },
    "scripts": {
        "csfixer":     "php .github/workflows/vendor/bin/php-cs-fixer fix --verbose --config=.github/workflows/php_cs.dist --stop-on-violation",
        "csfixerdry":  "php .github/workflows/vendor/bin/php-cs-fixer fix --verbose --config=.github/workflows/php_cs.dist --stop-on-violation --dry-run",
        "phan":        "php .github/workflows/vendor/bin/phan --debug",
        "phpmd":       "php .github/workflows/vendor/bin/phpmd src xml .github/workflows/phpmd.xml"
    }
}
